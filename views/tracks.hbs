<ul>
    {{#each tracks}}
        <li>
           {{this.name}}
            {{#if this.preview_url}}
                <audio controls src="{{this.preview_url}}" type="audio">
                    Your browser does not support the audio tag.
                </audio>
            {{else}}
                <p><b>No preview available! Sorry!</b></p>
            {{/if}}
        </li>
    {{/each}}
</ul>